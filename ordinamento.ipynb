   "metadata": {},
   "outputs": [],
   "source": [
    "def insertion_sort(array):\n",
    "    for posizione in range(1,len(array)):\n",
    "        key=array[posizione]\n",
    "        j=posizione-1\n",
    "        while j>=0 and array[j]>key:\n",
    "            array[j+1] = array[j]\n",
    "        array[j+1]=key\n",
    "        j-=j\n",
    "    return array\n",
    "    \n"
   ]
  }
